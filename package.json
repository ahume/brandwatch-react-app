  "description": "A starting point for creating a react app in Brandwatch",
  "engines": {
    "node": ">=8.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.css?$": "identity-obj-proxy"
    }
  },
  "main": "",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config webpack.static.config.js",
